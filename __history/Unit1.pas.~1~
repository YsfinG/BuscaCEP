unit Unit1;

interface

uses
   Vcl.Mask;

type
   TMaskEdit = class(Vcl.Mask.TMaskEdit)
   private
      procedure TelefoneMask;
   protected
      procedure DoEnter; override;
      procedure DoExit; override;
   end;

implementation

{ TMaskEdit }

procedure TMaskEdit.DoEnter;
begin
   inherited;

end;

procedure TMaskEdit.DoExit;
begin
   inherited;
   TelefoneMask;
end;

end.
